pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hai') {
            steps {
                echo 'Hai'
                echo 'Hello'
            }
        }
        stage('Test') {
            steps {
                echo 'Test Env'
            }
        }
    }
}
